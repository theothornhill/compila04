program runme
begin
  struct Complex {  Real : float; Imag : float  };
  
  var dummy : Complex;
  
  proc Add ( a : Complex, b : Complex ) : Complex 
  begin
      var retval : Complex
  in
      retval := new Complex;
      retval.Real := a.Real + b.Real;
      retval.Imag := a.Imag + b.Imag;
      return retval
  end;
 
  proc Max ( a : int , b : int ) : int 
  begin
      var res : int
  in
      if a > b 
      then  
          res := a
      else
         res := b
      fi;
      return res
  end;
  
  proc printCmplx( pr : Complex ) 
  begin
  in
      printstr("Real ");
      printfloat((pr).Real);
      printline("");
      printstr("Imag ");
      printfloat((pr).Imag);
      printline("")
  end;
  
